      "source": [
        "# Build LDA model - 12 topics\n",
        "lda_12_model = LDA(corpus=corpus,\n",
        "                  id2word=id2word,         \n",
        "                  num_topics=12,           # Number of topics\n",
        "                  random_state=100,        # Random \n",
        "                  update_every=1,          # Determines how often the model parameters should be updated\n",
        "                  chunksize=110,           # Increase chunk size will speed up training (number of docs to load into memory at a time and process E step of EM)\n",
        "                  passes=10,               # Total number of training passes through the entire corpus\n",
        "                  iterations=50,           # the maximum iterations over each document to reach convergence — limiting this means that some documents may not converge in time.\n",
        "                  alpha='auto',\n",
        "                  per_word_topics=True)"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "-EWAQby_yNF4",
        "outputId": "1a08fe76-3536-41e9-cabc-1163de3d9d99"
      },
      "source": [
        "# Print the Keyword in the 12 topics\n",
        "doc_lda = lda_12_model[corpus]\n",
        "lda_12_model.print_topics(12)"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
